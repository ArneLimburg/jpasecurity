<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
                 version="1.0">
  <package>net.sf.jpasecurity.model</package>
  <entity class="ParentTestBean" access="FIELD">
    <attributes>
      <id name="id">
        <generated-value/>
      </id>
      <basic name="name"/>
      <version name="version"/>
      <one-to-many name="children">
        <cascade>
          <cascade-all/>
        </cascade>
      </one-to-many>
    </attributes>
  </entity>
  <entity class="ChildTestBean" access="FIELD">
    <attributes>
      <id name="id">
        <generated-value/>
      </id>
      <basic name="name"/>
    </attributes>
  </entity>
</entity-mappings>